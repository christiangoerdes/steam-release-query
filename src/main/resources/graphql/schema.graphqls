type Query {
    releases: [Release]
    releasesByName(name : String!): [Release]
    releasesByRating(rating : String!): [Release]
    releasesBy(field : String): [Release]
}

type Release {
    name: String!
    link: String
    release: String
    peakPlayers: Int
    positiveReviews: Int
    negativeReviews: Int
    totalReviews: Int
    rating: String
}